@model IEnumerable<GoodToCode.Entities.Web.Models.SettingType>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SettingTypeKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SettingTypeName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SettingTypeKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SettingTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SettingTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SettingTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SettingTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
