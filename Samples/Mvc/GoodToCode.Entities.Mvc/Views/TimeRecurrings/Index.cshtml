@model IEnumerable<GoodToCode.Entities.Web.Models.TimeRecurring>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TimeRecurringKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BeginDay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BeginTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Interval)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeCycleKeyNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TimeRecurringKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeginDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeginTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Interval)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeCycleKeyNavigation.TimeCycleDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TimeRecurringId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TimeRecurringId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TimeRecurringId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
