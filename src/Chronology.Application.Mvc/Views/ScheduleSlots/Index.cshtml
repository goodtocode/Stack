@model IEnumerable<GoodToCode.Shared.Models.ScheduleSlot>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ScheduleSlotKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifiedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScheduleKeyNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SlotKeyNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleSlotKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleKeyNavigation.ScheduleDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SlotKeyNavigation.SlotDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ScheduleSlotId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ScheduleSlotId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ScheduleSlotId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
